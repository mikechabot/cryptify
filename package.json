{
  "name": "cryptify",
  "version": "4.0.0",
  "description": "File-based encryption utility for Node.js",
  "bin": "./cli.js",
  "scripts": {
    "start": "node index",
    "build": "npm run clean && babel src -d lib",
    "cli": "babel-node ./src/cli.js",
    "test": "jest",
    "prepublish": "crlf --set=LF index.js",
    "postpublish": "crlf --set=CRLF index.js",
    "clean": "npm run clean:build && npm run clean:validate",
    "clean:build": "rm -rf ./lib",
    "clean:validate": "rm -rf ./validated-ciphers",
    "validate": "npm run build && babel-node ./validateCiphers.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikechabot/cryptify.git"
  },
  "keywords": [
    "encryption",
    "decryption",
    "file-based encryption",
    "crypto",
    "Node.js encryption",
    "javascript",
    "node.js",
    "node"
  ],
  "author": "Mike Chabot",
  "license": "GPL-2.0-only",
  "bugs": {
    "url": "https://github.com/mikechabot/cryptify/issues"
  },
  "homepage": "https://github.com/mikechabot/cryptify#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "crlf": "1.1.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^25.3.0"
  },
  "dependencies": {
    "commander": "^5.0.0"
  }
}
